!URxvt.font: xft:Gohu GohuFont:pixelsize=20
! URxvt.font: xft:Comic Code Ligatures:pixelsize=14
! URxvt.font: xft:monoMMM_5:pixelsize=14
! URxvt.font: xft:ProggyClean Nerd Font:size=12
! URxvt.boldFont: xft:ProggyClean Nerd Font:size=12
! URxvt.font: xft:Monocraft:style=Regular:size=12
! URxvt.boldFont: xft:Monocraft:style=Regular:size=12
URxvt.font: xft:UbuntuMono Nerd Font:style=Regular:size=12
URxvt.boldFont: xft:UbuntuMono Nerd Font:style=Bold:size=12
URxvt.geometry:111x35
URxvt.perl-ext-common: "default,resize-font,52-osc,selection-to-clipboard,-searchable-scrollback"
! URxvt.clipboard.autocopy: true
! URxvt.keysym.M-c: pearl:clipboard:copy
! URxvt.keysym.M-v: pearl:clipboard:paste

URxvt.internalBorder: 5
URxvt.cursorBlink: True
URxvt.scrollBar: false

URxvt*depth: 32
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show
URxvt.iso14755: false
URxvt.iso14755_52: false


